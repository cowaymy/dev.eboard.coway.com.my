oLink\"\n            :ripple=\"false\"\n            class=\"ListItemClass\"\n          >\n            <v-list-item-icon>\n              <v-icon>{{ sublink.icon }}</v-icon>\n            </v-list-item-icon>\n\n            <v-list-item-title class=\"ListItemClass\" v-text=\"link.menuName\" />\n          </v-list-item>\n\n          <v-list-group\n            active-class=\"SelectedTile-active\"\n            v-else\n            :key=\"link.menuName\"\n            :prepend-icon=\"link.icon\"\n            :value=\"false\"\n          >\n            <template v-slot:activator>\n              <v-list-item-title>{{ link.text }}</v-list-item-title>\n            </template>\n\n            <v-list-item\n              class=\"SelectedTile\"\n              v-for=\"sublink in link.sublinks\"\n              :to=\"sublink.toLink\"\n              :key=\"sublink.text\"\n            >\n              <v-list-item-icon class=\"ListItemImgClass\">\n                <v-icon> {{ sublink.icon }}</v-icon>\n              </v-list-item-icon>\n              <v-list-item-title class=\"ListItemClass\">{{\n                sublink.text\n              }}</v-list-item-title>\n            </v-list-item>\n          </v-list-group>\n        </div>\n      </v-list>\n\n      <template v-slot:append>\n        <div class=\"pa-2\">\n          <v-btn block>\n            <v-icon @click=\"logoutUser\"> fas fa-sign-out-alt</v-icon>\n            Logout\n          </v-btn>\n        </div>\n      </template>\n    </v-navigation-drawer>\n\n    <v-app-bar height=\"50\" app>\n      <span> &nbsp;</span>&nbsp;\n      <v-app-bar-nav-icon @click=\"drawer = !drawer\"></v-app-bar-nav-icon>\n      <v-toolbar-title>eTtrust ePapan</v-toolbar-title>\n      <v-spacer></v-spacer>\n\n      <!-- Right  Notification & User Dropdown -->\n      <div class=\"d-flex align-center right-row\">\n        <AppBarShortcuts></AppBarShortcuts> &nbsp; &nbsp;\n        <AppBarNotification></AppBarNotification> &nbsp;\n        <AppBarUserMenu> </AppBarUserMenu>\n      </div>\n    </v-app-bar>\n\n    <v-main>\n      <div\n        v-if=\"$store.state.userInfo && $store.state.userInfo.userTypeId == '1'\"\n        class=\"app-content-container boxed-container pa-6\"\n      >\n        <eMdaltor open=\"false\"></eMdaltor>\n      </div>\n\n      <template v-if=\"spinnerStatus\">\n        <spinner :loading=\"spinnerStatus\"> </spinner>\n      </template>\n      <v-container fluid>\n        <keep-alive :include=\"['LoginForm']\">\n          <transition name=\"fab-transition\" appear>\n            <router-view></router-view>\n          </transition>\n        </keep-alive>\n\n        <!-- <div class=\"wrapper\">\n\t\t\t\t\t<div class=\"content\">\n\t\t\t\t\t\t<spinner :loading=\"spinnerStatus\"> </spinner>\n\t\t\t\t\t</div>\n\t\t\t\t</div> -->\n      </v-container>\n    </v-main>\n\n    <v-footer app inset color=\"transparent\" absolute height=\"56\" class=\"px-0\">\n      <div class=\"boxed-container w-full\">\n        <div class=\"mx-6 d-flex justify-space-between\">\n          <span>\n            &copy; 2022\n            <a href=\"\" class=\"text-decoration-none\" target=\"_blank\"\n              >COWAY ITS&O</a\n            ></span\n          >\n        </div>\n      </div>\n    </v-footer>\n  </v-app>\n</template>\n<script>\nimport store from \"@/store\";\nimport AppBarNotification from \"../../components/appBar/AppBarNotification.vue\";\nimport AppBarUserMenu from \"../../components/appBar/AppBarUserMenu.vue\";\nimport AppBarShortcuts from \"../../components/appBar/AppBarShortcuts\";\n\nexport default {\n  components: {\n    AppBarNotification,\n    AppBarUserMenu,\n    AppBarShortcuts,\n  },\n\n  computed: {\n    isUserLogin() {\n      return this.$store.getters.isLogin;\n    },\n\n    userInfo() {\n      return this.$store.state.userInfo;\n    },\n    \n    links(){\n      return this.$store.state.memuList;\n    },\n  },\n\n  data() {\n    return {\n      drawer: null,\n      spinnerStatus: false,\n    };\n  },\n\n  methods: {\n    logoutUser() {\n      this.$store.commit(\"clearUserInfo\");\n      this.$router.push(\"/\");\n    },\n    \n    asyncfetchDataList() { \n      console.log('log.....')\n      store.dispatch(\"getMemuList\");\n    },\n  },\n\n  created(){\n          this.asyncfetchDataList();\n  },\n};\n</script>\n\n<style>\n.ListItemClass {\n  color: #f5f5f5;\n}\n.ListItemImgClass {\n  color: #d9f00f;\n}\n\n.SelectedTile:hover {\n  border-radius: 4px;\n  background: #aaabac;\n}\n\n.SelectedTile-active {\n  background: #fff;\n  border-top-left-radius: 30px;\n  border-bottom-left-radius: 30px;\n  text-decoration: none;\n}\n\n@media all and (display-mode: standalone) {\n  #pwa {\n    display: block;\n  }\n}\n\n@media all and (display-mode: browser) {\n  #pwa {\n    display: none;\n  }\n}\n</style>\n"]}]}