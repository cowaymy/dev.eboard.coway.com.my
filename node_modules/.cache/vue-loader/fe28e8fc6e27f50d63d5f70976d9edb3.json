/VueRolling>\n\n              <!-- <Roller\n\t\t\t\t\t\t\t\ttext=\"vue-roller\"\n\t\t\t\t\t\t\t\t:charList=\"234234234\"\n\t\t\t\t\t\t\t\t:transition=\"2\"\n\t\t\t\t\t\t\t\t:isStatic=\"true\"\n\t\t\t\t\t\t\t\tclass=\"roller\"\n\t\t\t\t\t\t\t></Roller> -->\n              <!-- <v-btn small color=\"primary\" @click=\"isDialogVisible = false\">\n\t\t\t\t\t\t\t\tView Sales\n\t\t\t\t\t\t\t</v-btn> -->\n\n              <div>\n                <DashboardDialog\n                  :buttonTitle=\"`View More`\"\n                  :statTitle=\"`Beat My Best Record List`\"\n                />\n              </div>\n            </div>\n          </div>\n        </v-card-text>\n      </v-col>\n\n      <v-col cols=\"4\">\n        <v-img\n          contain\n          height=\"180\"\n          width=\"159\"\n          :src=\"\n            require(`@/assets/images/misc/triangle-${\n              $vuetify.theme.dark ? 'dark' : 'light'\n            }.png`)\n          \"\n          class=\"greeting-card-bg\"\n        ></v-img>\n        <v-img\n          contain\n          height=\"108\"\n          max-width=\"83\"\n          class=\"greeting-card-trophy\"\n          src=\"@/assets/images/misc/trophy.png\"\n        ></v-img>\n      </v-col>\n      <template v-if=\"isDialogVisible\">\n        <DashboardDialog />\n      </template>\n\n      <template v-else> </template>\n    </v-row>\n  </v-card>\n</template>\n<script>\nimport VueRolling from \"vue-roller\";\nimport codyApi from \"../../../api/codyApi.js\";\nimport DashboardDialog from \"../../comm/DialogBottomScreen_cody.vue\";\n\nexport default {\n  components: { VueRolling, DashboardDialog },\n\n  methods: {\n    async callApiBestRecord() {\n      try {\n        const userInfo = this.$store.state.userInfo;\n        return await codyApi.getCodyBestRecord(userInfo);\n      } catch (error) {\n        console.log(error);\n      }\n    },\n  },\n  data() {\n    const data = {\n      bestRecord: 0,\n    };\n    const isDialogVisible = false;\n    return { data, isDialogVisible };\n  },\n\n  created() {\n    this.callApiBestRecord().then((request) => {\n      this.data = {\n        userNo: this.$store.state.userInfo.userFullName,\n        userName: this.$store.state.userInfo.userName,\n        bestRecord: request.data.data[0].BEST_RECORD,\n      };\n    });\n  },\n};\n</script>\n\n<style lang=\"scss\" scoped>\n.greeting-card {\n  position: relative;\n  .greeting-card-bg {\n    position: absolute;\n    bottom: 0;\n    right: 0;\n  }\n  .greeting-card-trophy {\n    position: absolute;\n    bottom: 10%;\n    right: 8%;\n  }\n}\n// rtl\n.v-application {\n  &.v-application--is-rtl {\n    .greeting-card-bg {\n      right: initial;\n      left: 0;\n      transform: rotateY(180deg);\n    }\n    .greeting-card-trophy {\n      left: 8%;\n      right: initial;\n    }\n  }\n}\n\n.font-weight-semibold {\n  font-weight: 600 !important;\n}\n.text-5xl {\n  font-size: 3rem !important;\n  line-height: 1 !important;\n}\n\n.text-4xl {\n  font-size: 2rem !important;\n  line-height: 1 !important;\n}\n\n.text-2xl {\n  font-size: 1.5rem !important;\n  line-height: 2rem !important;\n}\n.text-base {\n  font-size: 1rem !important;\n  line-height: 1.5rem !important;\n}\n\n.roller .rollerBlock {\n  font-size: 1rem !important;\n  font-weight: 600 !important;\n}\n</style>\n"]}]}