ult.outcome === 'accepted') {\r\n\t\t\t\t\t\tconsole.log('User accepted the install prompt');\r\n\t\t\t\t\t\tsavePWACookie(true);\r\n\t\t\t\t\t} else {\r\n\t\t\t\t\t\tconsole.log('User dismissed the install prompt');\r\n\t\t\t\t\t}\r\n\t\t\t\t\tthis.deferredPrompt = null;\r\n\t\t\t\t});\r\n\t\t\t} catch (e) {\r\n\t\t\t\tconsole.log(e);\r\n\t\t\t\tconsole.log('pwa :::', getPWAFromCookie());\r\n\t\t\t\tconsole.log('이미 설치 되어 있거나 지원하지 않는 브라우저 !!!');\r\n\t\t\t}\r\n\t\t},\r\n\t},\r\n\r\n\twatch: {\r\n\t\tisInstallPWA(val) {\r\n\t\t\tconsole.log('======');\r\n\t\t\tconsole.log('[' + val + ']');\r\n\t\t\tconsole.log('======');\r\n\t\t\tif (val == 'true') {\r\n\t\t\t\tconsole.log('val isInstallPWA ::::', val);\r\n\t\t\t\tthis.closeDialog();\r\n\t\t\t\tconsole.log('close dialog::::', val);\r\n\t\t\t} else {\r\n\t\t\t\tconsole.log('val isInstallPWA ::::', val);\r\n\t\t\t\tthis.openDialog();\r\n\t\t\t\tconsole.log('open  dialog::::', val);\r\n\t\t\t}\r\n\t\t},\r\n\t},\r\n};\r\n</script>\r\n<sctyle lang=\"scss\">\r\n.reminder-modal-title-logo {\r\n\twidth: 50px;\r\n\tpadding-top: 10px;\r\n\tmargin-left: 9%;\r\n}\r\n.reminder-modal-title-logo-sm {\r\n\twidth: 80px;\r\n\tpadding-top: 10px;\r\n\tmargin-left: 7%;\r\n}\r\n\r\n.mazda-modal-close-button {\r\n\twidth: 30px;\r\n\tpadding-top: 25px;\r\n}\r\n\r\n.mazda-modal-close-button-sm {\r\n\twidth: 50px;\r\n\tpadding-top: 25px;\r\n}\r\n</sctyle>\r\n"]}]}