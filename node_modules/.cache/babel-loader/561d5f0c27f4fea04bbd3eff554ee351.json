n\n      //return Promise.reject('1');\n    }\n  );\n\n  return instance;\n}\n\nexport function createRPTInstance() {\n  const instance = axios.create({\n    baseURL: \"http://172.16.252.100:8080/report/\", //\n    headers: {\n      \"Content-Type\": \"application/x-www-form-urlencoded\",\n    },\n  });\n\n  return instance;\n}\n"]}]}