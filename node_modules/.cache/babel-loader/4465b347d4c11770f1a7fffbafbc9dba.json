  <v-card class=\"body\">\r\n          <v-row class=\"ma-0 h-full\">\r\n            <v-col\r\n              cols=\"12\"\r\n              order=\"1\"\r\n              order-lg=\"1\"\r\n            >\r\n              <v-card-title class=\"d-flex font-weight-semibold text--primary\">{{item.SUBJECT}}</v-card-title>\r\n            \r\n              <v-card-text>\r\n                <v-divider></v-divider>\r\n              </v-card-text>\r\n              <v-card-text class=\"d-flex justify-center\">\r\n                <div class=\"d-flex align-center rounded-lg\" >\r\n                    <v-avatar\r\n                        size=\"40\"\r\n                        rounded\r\n                        class=\"v-avatar-light-bg\"\r\n                    >\r\n                        <v-img\r\n                        contain\r\n                        :src=userImg\r\n                        ></v-img>\r\n                    </v-avatar>\r\n                    <div class=\"create_info\">\r\n                        <p> <span class=\"font-weight-semibold\">{{item.createUser}} &nbsp; create by <b class=\"font-weight-semibold\">{{item.branchName}}</b></span></p>\r\n                        <p> <span class=\"font-weight-semibold\">{{item.CRT_DT }} </span> <span class=\"create_bar\">|</span>  <span class=\"create_read font-weight-semibold\"> READ &nbsp; {{item.rcnt}}  </span></p>\r\n                    </div>\r\n                </div>\r\n                \r\n               \r\n                <div class=\"ms-auto\">\r\n                    <v-divider class=\"my-2\"></v-divider>\r\n                </div> \r\n            </v-card-text>\r\n            <v-card-text>\r\n                <v-divider></v-divider>\r\n            </v-card-text>\r\n              \r\n            <v-card-text>\r\n                <quill-editor\r\n                    ref=\"myQuillEditor\"\r\n                    v-model=\"contents\"\r\n                    :options=\"editorOption\"\r\n                />\r\n\r\n\r\n\r\n            \r\n            </v-card-text>\r\n\r\n            <v-card-actions>\r\n                <v-spacer></v-spacer>\r\n                     <v-btn  color=\"info\" @click=\"goBack()\" >\r\n                        <v-icon> \r\n                        {{ icons.mdiBackburger }}\r\n                        </v-icon>\r\n                        <span>LIST</span>\r\n                    </v-btn>\r\n            </v-card-actions>\r\n       \r\n          \r\n\r\n            </v-col>\r\n          \r\n          </v-row>\r\n        </v-card>\r\n  </template>\r\n\r\n<script>\r\n\r\nimport themeConfig from \"../../../themeConfig\";\r\nimport {\r\n  mdiChevronUp,\r\n  mdiChevronDown,\r\n  mdiCartPlus,\r\n  mdiShareVariantOutline,\r\n  mdiLockOpenOutline,\r\n  mdiStarOutline,\r\n  mdiAccountOutline,\r\n  mdiTrendingUp,\r\n  mdiHelpCircleOutline,mdiBackburger\r\n} from '@mdi/js'\r\n\r\nimport baseApi from \"../../api/index.js\";\r\nimport utils from \"../../utils/utils.js\";\r\n\r\nimport 'quill/dist/quill.core.css'\r\nimport 'quill/dist/quill.snow.css'\r\nimport 'quill/dist/quill.bubble.css'\r\n\r\nimport { quillEditor } from 'vue-quill-editor'\r\n\r\nexport default {\r\n  components: {\r\n    quillEditor\r\n  },\r\n  data() {\r\n    return {\r\n          appLogo: themeConfig.app.logo,\r\n          item : {} ,\r\n          contents :'',\r\n          userImg: \"\", \r\n          icons: {\r\n                mdiChevronUp,\r\n                mdiChevronDown,\r\n                mdiCartPlus,\r\n                mdiShareVariantOutline,\r\n                mdiLockOpenOutline,\r\n                mdiStarOutline,\r\n                mdiAccountOutline,\r\n                mdiTrendingUp,\r\n                mdiHelpCircleOutline,mdiBackburger\r\n      },\r\n      editorOption: {\r\n        readOnly: \"true\",\r\n        modules: {\r\n            toolbar: null\r\n        },\r\n      }\r\n    };\r\n  }, \r\n\r\n  methods: {\r\n    async selectNotification() {\r\n        if( this.$route.query.id != undefined){\r\n            const obj ={ID:this.$route.query.id}\r\n            const data = await baseApi.getNotification(obj);\r\n            if(data.data.dataList){\r\n                this.item = data.data.dataList[0];\r\n                this.contents = this.unescapeHTML(data.data.dataList[0].CONTENTS);\r\n                //this.contents = data.data.dataList[0].CONTENTS;\r\n\r\n                    // ArrayBuffer를 텍스트로 디코딩\r\n        \r\n                this.userImg = `https://epapanapis.malaysia.coway.do/users/${data.data.dataList[0].imgPath}`\r\n\r\n                this.updateHitNotification();\r\n            }\r\n        } \r\n    },\r\n\r\n    \r\n    async updateHitNotification() {\r\n        if( this.$route.query.id != undefined){\r\n            const obj ={ID:this.$route.query.id , userName :  this.$store.state.userName}\r\n            const data = await baseApi.updateHitNotification(obj);\r\n        } \r\n    },\r\n \r\n    unescapeHTML(str){\r\n        return utils.unescapeHTML(str);\r\n    },\r\n    goBack() {\r\n      this.$router.go(-1); // 또는 this.$router.back()을 사용할 수 있습니다.\r\n    },\r\n\r\n    onEditorBlur(quill) {\r\n        console.log('editor blur!', quill)\r\n      },\r\n      onEditorFocus(quill) {\r\n        console.log('editor focus!', quill)\r\n      },\r\n      onEditorReady(quill) {\r\n        console.log('editor ready!', quill)\r\n      },\r\n      onEditorChange({ quill, html, text }) {\r\n        console.log('editor change!', quill, html, text)\r\n        this.contents = quill\r\n      }\r\n  },\r\n\r\n  mounted () {\r\n    this.ID = this.$route.query.id;\r\n    this.selectNotification();\r\n  },\r\n  computed: {\r\n      editor() {\r\n        return this.$refs.myQuillEditor.quill\r\n      },\r\n\r\n    },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n    .create_info p {\r\n        margin: 0;\r\n        margin-left: 10px;\r\n    }\r\n    .create_read {\r\n        color: blue !important;\r\n        /*font-size: 3 */\r\n    }\r\n    .create_bar {\r\n        color: rgb(243, 10, 10) !important;\r\n        font-weight: bold;\r\n    } \r\n\r\n</style>\r\n"],"sourceRoot":"src/views/notification"}]}