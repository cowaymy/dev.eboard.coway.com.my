      .getDetailNewNotification(userInfo)\n          .then((response) => {\n            console.log(response.data.dataList);\n            commit(\"SET_DATA_ITEM_LIST\", response.data.dataList);\n            commit(\"SET_DATA_COUNT\");\n            resolve(response);\n          })\n          .catch((error) => reject(error))\n          .finally(() => {\n            bus.$emit(\"end:spinner\");\n          });\n      });\n    },\n  },\n};\n"]}]}